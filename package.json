{
  "name": "lightning-ui",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "chrome >= 79",
    "firefox >= 86",
    "last 2 major versions",
    ">0.5%",
    "not iOS < 14",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.64",
    "@mui/material": "^5.2.7",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "lodash": "^4.17.21",
    "notistack": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.7",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@craco/craco": "^6.4.3",
    "@cypress/react": "^5.12.1",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-table": "^7.7.9",
    "cypress": "^9.4.1",
    "eslint-plugin-compat": "npm:@yurijmikhalevich/eslint-plugin-compat@4.0.2-fix1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "storybook-addon-designs": "^6.2.1"
  },
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "lint": "eslint --cache --max-warnings=0 . && prettier -c .",
    "lint:fix": "eslint --cache --max-warnings=0 . --fix && prettier -w .",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "test:clear": "cypress cache clear",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*": "yarn lint:fix"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "react-app",
      "plugin:compat/recommended"
    ],
    "ignorePatterns": [
      "node_modules/**",
      "build/**"
    ],
    "settings": {
      "lintAllEsApis": true
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
